    "@angular/forms": "^19.0.3",
    "@angular/platform-browser": "^19.0.3",
    "@angular/platform-browser-dynamic": "^19.0.3",
    "@angular/router": "^19.0.3",
    "chart.js": "^4.4.7",
    "jest-mock": "^29.7.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0",
    "@opentelemetry/sdk-trace-web": "^1.9.0",
    "@opentelemetry/sdk-trace-base": "^1.9.0"
